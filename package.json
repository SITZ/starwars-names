  "author": "Sitesh Shrivastava <siteshshrivastava@gmail.com> (http://www.siteshshrivastava.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SITZ/starwars-names/issues"
  },
  "homepage": "https://github.com/SITZ/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "ghooks": "1.0.3",
    "mocha": "2.3.4",
    "semantic-release": "^4.3.5"
